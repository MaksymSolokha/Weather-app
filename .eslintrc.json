{
	"env": {
		"browser": true,
		"commonjs": true,
		"es2021": true
	},
	"extends": ["plugin:react/recommended", "airbnb"],
	"overrides": [],
	"parserOptions": {
		"ecmaVersion": "latest"
	},
	"plugins": ["react"],
	"rules": {
		"indent": [2, "tab", { "SwitchCase": 1, "VariableDeclarator": 1 }],
		"no-tabs": 0,
		"react/prop-types": 0,
		"react/jsx-indent": [2, "tab"],
		"react/jsx-indent-props": [2, "tab"]
	}
}
